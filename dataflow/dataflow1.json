{
	"name": "dataflow1",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "DROPBOX_API",
						"type": "DatasetReference"
					},
					"name": "source1"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "Json_API_Response_dropb_list_files",
						"type": "DatasetReference"
					},
					"name": "sink1"
				}
			],
			"transformations": [],
			"scriptLines": [
				"parameters{",
				"     jsoon_file_name_api_response as string ('response_dropbx_api_list_files.json')",
				"}",
				"source(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     httpMethod: 'POST',",
				"     body: '\"include_deleted\": false,",
				" \"include_has_explicit_shared_members\": false,",
				"    \"include_media_info\": false,",
				"    \"include_mounted_folders\": true,",
				"    \"include_non_downloadable_files\": true,",
				"    \"path\": \"/DATA\",",
				"    \"recursive\": false',",
				"     timeout: 30,",
				"     headers: ['Content-Type' -> 'application/json'],",
				"     responseFormat: ['type' -> 'json', 'documentForm' -> 'documentPerLine']) ~> source1",
				"source1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     filePattern:($jsoon_file_name_api_response),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> sink1"
			]
		}
	}
}