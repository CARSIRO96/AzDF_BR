{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-csr961"
		},
		"AzureBlobStorage1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage1'"
		},
		"AzureSqlDatabase1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSqlDatabase1'"
		},
		"DOPB OAUTH_clientSecret": {
			"type": "secureString",
			"metadata": "Secure string for 'clientSecret' of 'DOPB OAUTH'"
		},
		"DOPB OAUTH_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://www.dropbox.com"
		},
		"DOPB OAUTH_properties_typeProperties_clientId": {
			"type": "string",
			"defaultValue": "kpamkpg3iqd0u8z"
		},
		"GIT_CSV_LINK_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://raw.githubusercontent.com/CARSIRO96/DATA_TRAIN/master/CSVs/empleados.csv"
		},
		"RestService1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://api.dropboxapi.com/"
		},
		"adftest_properties_typeProperties_existingClusterId": {
			"type": "string",
			"defaultValue": "0202-195510-ujd6538b"
		},
		"akvtest_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://SQLServerCTPKeys.vault.azure.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-csr961_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlobStorage1_connectionString": {
						"value": "[parameters('AzureBlobStorage1_connectionString')]"
					},
					"AzureSqlDatabase1_connectionString": {
						"value": "[parameters('AzureSqlDatabase1_connectionString')]"
					},
					"DOPB OAUTH_clientSecret": {
						"value": "[parameters('DOPB OAUTH_clientSecret')]"
					},
					"DOPB OAUTH_properties_typeProperties_url": {
						"value": "[parameters('DOPB OAUTH_properties_typeProperties_url')]"
					},
					"DOPB OAUTH_properties_typeProperties_clientId": {
						"value": "[parameters('DOPB OAUTH_properties_typeProperties_clientId')]"
					},
					"GIT_CSV_LINK_properties_typeProperties_url": {
						"value": "[parameters('GIT_CSV_LINK_properties_typeProperties_url')]"
					},
					"RestService1_properties_typeProperties_url": {
						"value": "[parameters('RestService1_properties_typeProperties_url')]"
					},
					"akvtest_properties_typeProperties_baseUrl": {
						"value": "[parameters('akvtest_properties_typeProperties_baseUrl')]"
					},
					"adftest_properties_typeProperties_existingClusterId": {
						"value": "[parameters('adftest_properties_typeProperties_existingClusterId')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}